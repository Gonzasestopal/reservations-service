[flake8]
max-line-length = 120
ignore =
    WPS117
    WPS202
    WPS210
    WPS226
    WPS204
    WPS305
    WPS432
    WPS440
    WPS441
    D100
    D101
    D102
    D103
    D104
    D105
    D106
    D107
    DAR101
    DAR201
per-file-ignores =
    tests/**:F401
    seeds.py: D103
    interface.py: WPS306, WPS420, WPS463
format = wemake
